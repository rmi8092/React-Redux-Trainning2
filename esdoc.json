{
    "source": "./src",
    "destination": "./doc",
    "includes": ["\\.(js|jsx)$"],
    "excludes": ["\/node_modules\/","\\.confing\\.(js|jsx)$"],    
    "experimentalProposal": {
        "classProperties": true,
        "objectRestSpread": true,
        "decorators": true,
        "doExpressions": true,
        "functionBind": true,
        "asyncGenerators": true,
        "exportExtensions": true,
        "dynamicImport": true
    }
}